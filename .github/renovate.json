{
    "extends": [
      "config:recommended",
      ":disableDependencyDashboard"
    ],
  "postUpdateOptions": [
    "gomodTidy"
  ],
    "packageRules": [
      {
      "description": "Disable updating minimum Go version: https://github.com/renovatebot/renovate/issues/16715",
      "matchManagers": ["gomod"],
      "matchDepTypes": ["golang"],
      "enabled": false
      }
    ]
  }
  